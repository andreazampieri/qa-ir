{
    "embeddings":"glove",
    "train_as":"classifier",
    "parser":"text",
    "model":{
        "name":"cnn",
        "params":{
            "static_emb": true,
            "hidden_size":1,
            "pad_idx":0,
            "qcnn":{
                "conv_size":100,
                "windows":[5],
                "activation":"tanh"
            },
            "acnn":{
                "conv_size":100,
                "windows":[5],
                "activation":"tanh"
            }
        }

    },
    "seed": 123,
    "patience": 25,
    "batch_size": 50,
    "optimizer":{
        "name":"Adadelta",
        "params":{
            "lr": 0.8
        }
    }
}
