{
    "embeddings":"glove",
    "train_as":"classifier",
    "parser":"text",
    "model":{
        "name":"cnn",
        "params":{
            "static_emb": true,
            "hidden_size":100,
            "pad_idx":0,
            "qcnn":{
                "conv_size":100,
                "windows":[5],
                "activation":"relu"
            },
            "acnn":{
                "conv_size":100,
                "windows":[5],
                "activation":"relu"
            }
        }

    },
    "seed": 123,
    "patience": 25,
    "batch_size": 50,
    "optimizer":[{
        "name":"Adadelta",
        "params":{
            "lr": 0.8
        }},
        [{
        "name":"Adadelta",
        "params":{
            "lr": 0.1
        }}
    ]
    }
}
