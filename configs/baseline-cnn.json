{
    "embeddings":"glove",
    "train_as":"classifier",
    "parser":"text",
    "model":{
        "name":"att-cnn",
        "params":{
            "static_emb": true,
            "hidden_size": 300,
            "pad_idx":0,
            "qcnn":{
                "conv_size": 4000,
                "window":5,
                "activation":"tanh"
            },
            "acnn":{
                "conv_size": 4000,
                "window":5,
                "activation":"tanh"
            }
        }

    },
    "seed": 123,
    "patience": 15,
    "batch_size": 50,
    "optimizer":[{
        "name":"Adam",
        "params":{
            "lr":0.002
        }
    },{
        "name":"Adadelta",
        "params":{
            "lr":0.1
        }
    }
    ]
}
