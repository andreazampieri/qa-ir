{
    "embeddings":"glove",
    "train_as":"classifier",
    "parser":"text",
    "model":{
        "name":"test-att",
        "params":{
            "static_emb": true,
            "hidden_size": 100,
            "pad_idx":0,
            "qcnn":{
                "conv_size": 100,
                "window":5,
                "activation":"relu"
            },
            "acnn":{
                "conv_size": 100,
                "window":5,
                "activation":"relu"
            }
        }

    },
    "seed": 123,
    "patience": 7,
    "batch_size": 50,
    "optimizer":{
        "name":"Adagrad",
        "params":{
            "lr": 0.01
        }
    }
}
