{
    "embeddings":"glove",
    "train_as":"classifier",
    "parser":"text",
    "model":{
        "name":"test-att-cnn",
        "params":{
            "static_emb": false,
            "hidden_size": 65,
            "pad_idx":0,
            "shared": false,
            "qcnn":{
                "conv_size": 120,
                "window":5,
                "activation":"tanh"
            },
            "acnn":{
                "conv_size": 120,
                "window":5,
                "activation":"tanh"
            }
        }

    },
    "seed": 123,
    "patience": 4,
    "batch_size": 50,
    "optimizer":{
        "name":"Adadelta",
        "params":{
            "lr": 0.1
        }
    }
}
